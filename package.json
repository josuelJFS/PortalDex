    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "babel-preset-expo": "~11.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "ts-jest",
